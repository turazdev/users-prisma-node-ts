{
    "name": "users-prisma-node-ts",
    "version": 2,
    "public": true,
    "rewrites":[
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "builds": [
        {
        "src": "src/server.ts",
        "use": "@vercel/node"
        },
        {
        "src": "public/**",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/((?!.*\\.\\w+$).*)",
        "dest": "src/server.ts"
        },
        {
        "src": "/(.+\\.[a-z]+)$",
        "dest": "/public/$1"
        }
    ]
}